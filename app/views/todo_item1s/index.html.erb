<mark><h1>Listing Todo Item1s</h1></mark>

<p> Maybe it is time I go to sleep </p>

<div class="table-responsive">
<table>
  <thead>
    <tr>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @todo_item1s.each do |todo_item1| %>
      <tr>
        <td><%= todo_item1.description %></td>
        <td><%= link_to 'Show', todo_item1 %></td>
        <td><%= link_to 'Edit', edit_todo_item1_path(todo_item1) %></td>
        <td><%= link_to 'Destroy', todo_item1, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>
